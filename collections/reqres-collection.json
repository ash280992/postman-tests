							"pm.test('Validate the status code', ()=>{",
							"    pm.response.to.have.status(200);",
							"})",
							"",
							"pm.test('Validate the json body', ()=>{",
							"    pm.expect(resJson).to.have.property('name');",
							"    pm.expect(resJson.name).to.eql(parJson.name);",
							"    pm.expect(resJson).to.have.property('job');",
							"    pm.expect(resJson.job).to.eql(parJson.job);",
