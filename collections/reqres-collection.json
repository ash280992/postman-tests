	"item": [
		{
			"name": "GET Users",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Validate Status Code', ()=>{",
							"    pm.response.to.have.status(200);",
							"})",
							"",
							"const dataJson = pm.response.json();",
							"pm.test('Validate user email', ()=>{",
							"    pm.expect(dataJson.data.email).contains('@');",
							"    pm.expect(dataJson.data.email).to.eql('janet.weaver@reqres.in');",
							"})",
							"pm.test('Validate response should have id', ()=>{",
							"    pm.expect(dataJson.data).to.have.property('id');",
							"    pm.expect(dataJson.data.id).to.eql(2);",
							"})",
							"",
							"pm.test('Validate response should have firstname and lastname', ()=>{",
							"    pm.expect(dataJson.data).to.have.property('first_name');",
							"    pm.expect(dataJson.data).to.have.property('last_name');",
							"})",
							"",
							"pm.test('Validate the response has support', ()=>{",
							"    pm.expect(dataJson.support).to.have.property('url');",
							"    pm.expect(dataJson.support).to.have.property('text');",
							"    pm.expect(dataJson.support.url).to.contain('reqres');",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.request.headers.add({",
							"    key: 'x-api-key',",
							"    value: 'reqres-free-v1'",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
